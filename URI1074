#include<stdio.h>
#include <stdlib.h>

int main(){
  
    int X,A,i,Y;
    scanf("%d",&A);
    
	for(i=1;i<=A;i++){
		scanf("%d",&Y);
		if(Y>0 && Y%2==0){
			printf("EVEN POSITIVE\n");
		}else if(Y<0 && Y%2==0){
			printf("EVEN NEGATIVE\n");
		}else if(Y==0){
			printf("NULL\n");
		}
		else if(Y>0 && Y%2==1){
			printf("ODD POSITIVE\n");
		}
		else if(Y<0 && Y%2==-1){
			printf("ODD NEGATIVE\n");
		}
		}
    return 0;
}
