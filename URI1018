#include <stdio.h>
#include <stdlib.h>
 
int main() {
 
    int VALOR,V100,V50,V20,V10,V5,V2,V1;
    
    scanf("%d",&VALOR);
    printf("%d\n",VALOR);
    
    V100=VALOR/100;
	VALOR=VALOR-(V100*100);
    V50=VALOR/50;
    VALOR=VALOR-(V50*50);
    V20=VALOR/20;
    VALOR=VALOR-(V20*20);
    V10=VALOR/10;
    VALOR=VALOR-(V10*10);
    V5=VALOR/5;
    VALOR=VALOR-(V5*5);
    V2=VALOR/2;
    VALOR=VALOR-(V2*2);
    V1=VALOR/1;
    
	printf("%d nota(s) de R$ 100,00\n",V100); 
	printf("%d nota(s) de R$ 50,00\n",V50);
	printf("%d nota(s) de R$ 20,00\n",V20);
	printf("%d nota(s) de R$ 10,00\n",V10);
	printf("%d nota(s) de R$ 5,00\n",V5);
	printf("%d nota(s) de R$ 2,00\n",V2);
	printf("%d nota(s) de R$ 1,00\n",V1);
	
    return 0;
}
