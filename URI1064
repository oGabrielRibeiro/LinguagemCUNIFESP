#include <stdio.h>

int main(){

    double B,X,Z;
    int A,i;
    A=0;
    X=0;
    Z=0;
    
    for(i=1;i<=6;i++){
        scanf("%lf",&B);
    if(B>=0){   
        A++;
        X+=B;     
        }
    }
	Z=X/A;
	
    printf("%d valores positivos\n",A);
    printf("%.1lf\n",Z);
    
    return 0;
}
